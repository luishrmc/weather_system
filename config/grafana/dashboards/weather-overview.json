{
    "id": null,
    "uid": "weather-overview",
    "title": "Weather Station Overview",
    "tags": [
        "weather",
        "sensors",
        "influxdb3"
    ],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "panels": [
        {
            "id": 1,
            "title": "Temperature & Humidity History",
            "type": "timeseries",
            "datasource": {
                "type": "influxdata-flightsql-datasource",
                "uid": "df6jfde329534b"
            },
            "gridPos": {
                "x": 0,
                "y": 0,
                "w": 16,
                "h": 8
            },
            "targets": [
                {
                    "refId": "A",
                    "queryText": "SELECT\n  time,\n  temperature_c AS \"Temperature (°C)\"\nFROM weather_data\nWHERE $__timeFilter(time)\nORDER BY time ASC",
                    "rawEditor": true,
                    "format": "time_series"
                },
                {
                    "refId": "B",
                    "queryText": "SELECT\n  time,\n  humidity_pct AS \"Humidity (%)\"\nFROM weather_data\nWHERE $__timeFilter(time)\nORDER BY time ASC",
                    "rawEditor": true,
                    "format": "time_series"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "lineWidth": 2,
                        "fillOpacity": 10,
                        "showPoints": "never"
                    }
                },
                "overrides": [
                    {
                        "matcher": {
                            "id": "byName",
                            "options": "Temperature (°C)"
                        },
                        "properties": [
                            {
                                "id": "color",
                                "value": {
                                    "mode": "fixed",
                                    "fixedColor": "orange"
                                }
                            },
                            {
                                "id": "unit",
                                "value": "celsius"
                            }
                        ]
                    },
                    {
                        "matcher": {
                            "id": "byName",
                            "options": "Humidity (%)"
                        },
                        "properties": [
                            {
                                "id": "color",
                                "value": {
                                    "mode": "fixed",
                                    "fixedColor": "blue"
                                }
                            },
                            {
                                "id": "unit",
                                "value": "percent"
                            },
                            {
                                "id": "custom.axisPlacement",
                                "value": "right"
                            }
                        ]
                    }
                ]
            },
            "options": {
                "legend": {
                    "displayMode": "table",
                    "placement": "bottom"
                },
                "tooltip": {
                    "mode": "single"
                }
            }
        },
        {
            "id": 2,
            "title": "Battery Voltage",
            "type": "gauge",
            "datasource": {
                "type": "influxdata-flightsql-datasource",
                "uid": "df6jfde329534b"
            },
            "gridPos": {
                "x": 16,
                "y": 0,
                "w": 8,
                "h": 8
            },
            "targets": [
                {
                    "refId": "A",
                    "queryText": "SELECT\n  time,\n  battery_voltage AS \"Battery Voltage\"\nFROM weather_data\nWHERE $__timeFilter(time)\nORDER BY time DESC\nLIMIT 1",
                    "rawEditor": true,
                    "format": "table"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "min": 3.0,
                    "max": 4.2,
                    "unit": "volt",
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "orange",
                                "value": 3.3
                            },
                            {
                                "color": "green",
                                "value": 3.7
                            }
                        ]
                    }
                },
                "overrides": []
            },
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "fields": "",
                    "values": false
                },
                "orientation": "auto",
                "showThresholdLabels": true,
                "showThresholdMarkers": true
            }
        },
